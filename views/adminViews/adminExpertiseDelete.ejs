  <%- include ("../partials/nav") %>
  <h1 class="mb-3">Expertise delete page</h1>
  <div class="container mt-3">
    <div class="row">
      <div class="cold-md-12 mt-3">
        <div class="alert alert-secondary">
          <p class="mb-3">
            Are you sure to delete expertise <b><%=expertise.expertiseName %>?</b>
