              <button class="btn btn-danger" type="submit">Yes</button>
              <a class="btn btn-secondary" href="/admin/expertises">No</a>
              
          </form>
        </div>
      </div>
    </div>
  </div>

  <%- include ("../partials/footer") %>
</body>
</html>
